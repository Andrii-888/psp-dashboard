{
  "name": "psp-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:format-guards": "rg -n \"toLocaleString\\(|Date\\.now\\(|Math\\.random\\(\" src/components src/app | rg -v \"src/app/api/\" && exit 1 || exit 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "verify": "npm run lint && npm run lint:format-guards && npm run typecheck && TZ=UTC NODE_ENV=production npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vercel/kv": "^3.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.561.0",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
